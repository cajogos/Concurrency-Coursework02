const MAX_DOCS = 3
range DOCS_RANGE = 0..MAX_DOCS

// DOC_ID

PRINTER = (
	acquire -> release -> PRINTER
).

User = (
	acquire -> print -> release -> User
).

Admin = (
	acquire -> service -> release -> Admin
).


|| PRINTER_SHARE = (
		user1: User || user2: User || admin: Admin
	|| { user1, user2, admin } :: PRINTER).