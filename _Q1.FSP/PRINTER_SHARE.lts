const MAX_DOCS = 3
range DOCS_RANGE = 0..MAX_DOCS


set SyncActions = { p.acquire, p.release }

// DOC_ID

PRINTER = (
	acquire -> release -> PRINTER
).

User = (
	p.acquire -> (
		print[DOCS_RANGE] -> p.release -> User
		| cancel[DOCS_RANGE] -> p.release -> User
	)
)+SyncActions.

Admin = (
	p.acquire -> service -> p.release -> Admin
)+SyncActions.


|| PRINTER_SHARE = (
	user1: User || user2: User || admin: Admin
	|| { user1, user2, admin }::p:PRINTER
).